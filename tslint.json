{
  "extends": ["tslint:recommended", "tslint-config-prettier"],
  "rules": {
    "interface-name": false,
    "member-access": [true, "no-public"],
    "variable-name": {
      "options": [
        "ban-keywords",
        "check-format",
        "allow-pascal-case",
        "allow-leading-underscore"
      ]
    }
  }
}
